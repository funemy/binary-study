<?xml version="1.0" standalone="no" ?>
<!DOCTYPE pov SYSTEM "/usr/share/cgc-docs/replay.dtd">
<pov>
<cbid>service</cbid>
<replay>
    <read><delim>\n</delim></read>
    <write><data>1\x0a</data></write>
    <read><delim>: </delim></read>
    <write><data>AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA\x0a</data></write>
    <read><delim>\n\n</delim></read>
    <write><data>recipe\x0a\x0a</data></write>
    <read><delim>: </delim></read>
    <write><data>\x0a</data></write>
    <read><delim>Exit\n</delim></read>
    <write><data>2\x0a</data></write>
    <read><delim>: </delim></read>
    <write><data>recipe\x0a</data></write>
    <read><delim>recipe? </delim></read>
    <write><data>\x0a</data></write>
    <delay>1000</delay>
</replay>
</pov>
